{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "no-use-before-define": 0,
      "linebreak-style": 0,
      "consistent-return": 0,
      "no-restricted-syntax": 0,
      "no-plusplus": 0,
      "no-param-reassign": 0,
      "no-unused-vars": 1,
      "prefer-const": 1,
      "no-console": 0,
      "camelcase": 0,
      "dot-notation": 0,
      "no-shadow": 0,
      "prefer-rest-params": 0,
      "prefer-template": 0
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
  